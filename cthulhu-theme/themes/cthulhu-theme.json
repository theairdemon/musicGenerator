{
    "name": "Cthulhu Dark Green - Python Focus",
    "type": "dark",
    "colors": {
        "editor.background": "#050A08",
        "editor.foreground": "#A8C5A1",
        "editorCursor.foreground": "#93DFB8",
        "editorLineNumber.foreground": "#3C6A62",
        "editor.selectionBackground": "#1C4035",
        "editor.selectionHighlightBackground": "#1C403550",
        "editor.findMatchBackground": "#417D6D",
        "editor.findMatchHighlightBackground": "#63B39630",
        "editor.wordHighlightBackground": "#417D6D30",
        "editor.wordHighlightStrongBackground": "#417D6D50",
        "editor.lineHighlightBackground": "#0C1311",
        "editorBracketMatch.background": "#3C6A62",
        "editorBracketMatch.border": "#215D4B",
        "editorWhitespace.foreground": "#16201C",
        "sideBar.background": "#050A08",
        "sideBar.foreground": "#4A7064",
        "activityBar.background": "#050A08",
        "activityBar.foreground": "#4A7064",
        "statusBar.background": "#050A08",
        "statusBar.foreground": "#4A7064",
        "titleBar.activeBackground": "#050A08",
        "titleBar.activeForeground": "#A8C5A1",
        "titleBar.inactiveBackground": "#0A1412",
        "titleBar.inactiveForeground": "#4A7064",
        "terminal.background": "#050A08",
        "terminal.foreground": "#A8C5A1",
        "panel.background": "#050A08",
        "panel.border": "#1A2B24",
        "dropdown.background": "#050A08",
        "dropdown.border": "#215D4B",
        "button.background": "#3C6A62",
        "button.foreground": "#A8C5A1",
        "badge.background": "#3C6A62",
        "badge.foreground": "#A8C5A1",
        "input.background": "#0C1311",
        "input.foreground": "#A8C5A1",
        "scrollbarSlider.background": "#3C6A6290",
        "scrollbarSlider.hoverBackground": "#3C6A62",
        "scrollbarSlider.activeBackground": "#63B396"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#4A7064",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#6BEFB2",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "variable",
                "meta.function-call.python"
            ],
            "settings": {
                "foreground": "#7EC18F"
            }
        },
        {
            "scope": "constant",
            "settings": {
                "foreground": "#86EAB5"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#5EB68E"
            }
        },
        {
            "scope": "string.quoted",
            "settings": {
                "foreground": "#5EB68E"
            }
        },
        {
            "scope": "support.function.builtin",
            "settings": {
                "foreground": "#93DFB8"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#93DFB8",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "variable.parameter",
            "settings": {
                "foreground": "#7EC18F"
            }
        },
        {
            "scope": "entity.name.class",
            "settings": {
                "foreground": "#B4FFDA",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "storage.type.class",
            "settings": {
                "foreground": "#6BEFB2",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "meta.function.decorator.python",
            "settings": {
                "foreground": "#6BEFB2",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "support.type",
            "settings": {
                "foreground": "#86EAB5"
            }
        },
        {
            "scope": "punctuation.separator",
            "settings": {
                "foreground": "#A8C5A1"
            }
        },
        {
            "scope": "operator",
            "settings": {
                "foreground": "#86EAB5"
            }
        },
        {
            "scope": "meta.brace",
            "settings": {
                "foreground": "#A8C5A1"
            }
        },
        {
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#D9534F",
                "background": "#5A2220",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "support.type.exception.python",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#B4FFDA"
            }
        },
        {
            "scope": [
                "entity.name.tag",
                "markup.heading"
            ],
            "settings": {
                "foreground": "#86EAB5",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "meta.function-call",
            "settings": {
                "foreground": "#93DFB8"
            }
        },
        {
            "scope": [
                "support.constant",
                "constant.language"
            ],
            "settings": {
                "foreground": "#D19A66"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#86EAB5"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name",
                "entity.name.tag.reference"
            ],
            "settings": {
                "foreground": "#7EC18F"
            }
        },
        {
            "scope": [
                "meta.attribute",
                "markup.underline.link"
            ],
            "settings": {
                "foreground": "#93DFB8"
            }
        },
        {
            "scope": [
                "constant.character.escape",
                "keyword.operator.assignment"
            ],
            "settings": {
                "foreground": "#93DFB8"
            }
        }
    ]
}